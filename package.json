{
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.511.0",
    "@aws-sdk/client-eventbridge": "^3.511.0",
    "@aws-sdk/client-sns": "^3.511.0",
    "@aws-sdk/client-sqs": "^3.511.0",
    "@aws-sdk/lib-dynamodb": "^3.511.0",
    "@middy/core": "^4.5.5",
    "@middy/http-json-body-parser": "^4.5.5",
    "@types/uuid": "^9.0.1",
    "mysql2": "^3.2.0",
    "uuid": "^9.0.0"
  },
  "name": "rimac-appointment-service",
  "version": "1.0.0",
  "description": "Servicio de gestión de citas médicas para RIMAC Seguros",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "start": "serverless offline start",
    "dev": "serverless offline start --stage dev",
    "clean": "rimraf .build && rimraf .serverless",
    "build": "npm run clean && tsc",
    "deploy:dev": "npm run build && serverless deploy --stage dev --verbose",
    "deploy:prod": "npm run build && serverless deploy --stage prod --verbose",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "migrate": "ts-node src/adapters/secondary/database/migrations/run.ts"
  },
  "keywords": [
    "rimac",
    "appointments",
    "medical",
    "serverless"
  ],
  "author": "RIMAC Seguros",
  "license": "UNLICENSED",
  "private": true
}
