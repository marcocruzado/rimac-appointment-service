{
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-sdk": "^2.1692.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "express": "^5.1.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.0.0",
    "serverless": "^4.12.0",
    "serverless-offline": "^12.0.0",
    "serverless-plugin-typescript": "^2.0.0",
    "ts-jest": "^29.5.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@middy/core": "^6.1.6",
    "@middy/http-json-body-parser": "^6.1.6",
    "@types/uuid": "^10.0.0",
    "middy": "^0.36.0",
    "mysql2": "^3.14.0",
    "uuid": "^11.1.0"
  },
  "name": "rimac-appointment-service",
  "version": "1.0.0",
  "description": "Servicio de gestión de citas médicas para RIMAC Seguros",
  "main": "index.js",
  "scripts": {
    "start": "serverless offline start",
    "dev": "serverless offline start --stage dev",
    "build": "tsc",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "migrate:mysql": "ts-node scripts/migrate-mysql.ts"
  },
  "keywords": [
    "rimac",
    "appointments",
    "medical",
    "serverless"
